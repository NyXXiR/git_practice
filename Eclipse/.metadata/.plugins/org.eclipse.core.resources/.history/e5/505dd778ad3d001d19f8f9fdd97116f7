
public class AuthApp {

	public static void main(String[] args) {

		System.out.println(args[0]);
		
		String id = "egoing";
		String inputID  = args[0];
		
		System.out.println("Hi.");
		
		if(inputID == id) {
			System.out.println("Welcome.");
		
		}else  {
			System.out.println("Who are you?");
		}
	}

}
