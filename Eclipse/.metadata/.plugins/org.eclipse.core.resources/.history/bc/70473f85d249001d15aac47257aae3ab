import java.util.Scanner;

public class Main {
	
public static void main(String[] args) {
	
	Scanner sc = new Scanner(System.in);
StringBuilder sb = new StringBuilder();

	int N = 5;
	

	for(int i=1; i<=N; i++) {
		while(N-i>0) {
		sb.append(" ");
		if(N-i<=0) {
		sb.append("*");
		}
		
		System.out.println(sb);
	}
		
}
}}
	

