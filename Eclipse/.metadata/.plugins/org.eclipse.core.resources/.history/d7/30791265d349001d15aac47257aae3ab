import java.util.Scanner;

public class Main {
	
public static void main(String[] args) {
	
	Scanner sc = new Scanner(System.in);
StringBuilder sb = new StringBuilder();

	int N = 10;
	int i = 1;
	
	while(N-i>0) {
	sb.append(" ");
	i++;
	if(N-i<=0) {
	sb.append("*");
	i++;
	}if(N==i) {
	System.out.println("\n");
		
	}
		
		System.out.println(sb);
	}
		
}
}

